{
	"name": "coffee-bot",
	"version": "1.0.0",
	"description": "coffee-bot",
	"main": "index.js",
	"dependencies": {
		"babel-cli": "^6.24.1",
		"babel-eslint": "^7.2.3",
		"babel-preset-flow": "^6.23.0",
		"husky": "^0.14.3",
		"keystone": "4.0.0-beta.5",
		"lint-staged": "^4.0.2",
		"prettier": "^1.5.3",
		"pug": "^2.0.0-beta11",
		"slack": "^8.3.1"
	},
	"scripts": {
		"postinstall": "yarn build",
		"lint": "eslint .",
		"build": "babel src -d lib --copyfiles",
		"precommit": "lint-staged",
		"start": "node index.js",
		"prettier": "prettier --single-quote --trailing-comma es5 --use-tabs --write"
	},
	"lint-staged": {
		"*.js": [
			"yarn prettier",
			"yarn lint",
			"git add"
		]
	},
	"author": "Ben Conolly",
	"license": "MIT",
	"devDependencies": {
		"dotenv": "^4.0.0",
		"eslint": "^4.3.0",
		"eslint-config-prettier": "^2.3.0",
		"babel-register": "^6.24.1",
		"eslint-config-keystone": "^3.0.0",
		"eslint-plugin-flowtype": "^2.35.0"
	},
	"engines": {
    "node": "8.x.x"
  }
}
